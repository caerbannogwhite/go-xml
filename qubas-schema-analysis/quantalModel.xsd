                          <label>@Html.LanguageString("DisplayName-AnalysisDef-GoodnessOfFit")</label>
                          <description>@Html.DescriptionString("Description-AnalysisDef-GoodnessOfFitQuantal")</description>
                    </UI></xs:appinfo></xs:annotation>
                  </xs:element>
                  <xs:element name="significanceOfSlope" type="ct:suitabilityCriteriaSignificanceOfSlopeType">
